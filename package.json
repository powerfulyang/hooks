{
  "name": "@powerfulyang/hooks",
  "version": "1.0.6",
  "description": "useful react hooks",
  "keywords": [
    "react-hooks"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "powerfulyang <i@powerfulyang.com>",
  "homepage": "https://github.com/powerfulyang/hooks#readme",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powerfulyang/hooks.git"
  },
  "scripts": {
    "prebuild": "rimraf dist && node ./check.js",
    "build": "run-p build:*",
    "build:source": "rollup -c",
    "build:types": "tsc -p tsconfig.types.json",
    "prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:js": "eslint --ext .ts,.tsx ./src --fix",
    "prepare": "npx husky install",
    "tsc-check": "tsc --noEmit",
    "pre-commit": "npm run tsc-check && lint-staged",
    "prepublishOnly": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/powerfulyang/hooks/issues"
  },
  "devDependencies": {
    "@powerfulyang/lint": "1.0.3",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-node-resolve": "13.2.1",
    "@rollup/plugin-typescript": "8.3.2",
    "@testing-library/react-hooks": "8.0.0",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "npm-run-all": "4.1.5",
    "react-test-renderer": "18.0.0",
    "rollup": "2.71.0",
    "tsconfig-paths-jest": "0.0.1"
  },
  "dependencies": {
    "@powerfulyang/utils": "1.3.12",
    "immer": "9.0.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "rxjs": "7.5.5"
  }
}
